pkgname=iana-etc
pkgver=20210611
pkgrel=1
pkgdesc='/etc/protocols and /etc/services provided by IANA'
url='https://www.iana.org/protocols'
arch=('any')
license=('')
install=iana-etc.install
source=("https://github.com/Mic92/iana-etc/releases/download/20210611/iana-etc-20210611.tar.gz")
md5sums=('f2854be57fe281e3ffc7364984467d2f')

package() {
	cd "${srcdir}/${pkgname}-${pkgver}"
	install -d "${pkgdir}/etc"
        cp services protocols ${pkgdir}/etc
}
